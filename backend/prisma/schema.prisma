generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Invoice {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  client_number   String @unique
  reference_month String 

  eletric_energy_quantity Float
  eletric_energy_price    Float

  sceee_energy_quantity_without_icms Float
  sceee_energy_price_without_icms    Float

  compensated_energy_quantity Float
  compensated_energy_price    Float

  municipal_public_lighting_price Float

  client_document_url String
}
